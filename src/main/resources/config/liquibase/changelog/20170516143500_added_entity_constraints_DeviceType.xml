                                 baseTableName="device_type_sensor_data"
                                 constraintName="fk_device_type_sensor_data_sensor_data_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sensor_data"/>
        
    </changeSet>
</databaseChangeLog>
