  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -e 'node_modules/**'",
    "lint:fix": "yarn run lint -- --fix",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p tsconfig-aot.json",
    "cleanup": "rimraf build/",
    "start": "yarn run webpack:dev",
    "serve": "yarn run start",
    "build": "yarn run webpack:prod && yarn run test",
    "webpack:build": "yarn run ngc && webpack --config webpack/webpack.vendor.js && webpack --config webpack/webpack.dev.js",
    "webpack:build:dev": "webpack --config webpack/webpack.dev.js",
    "webpack:build:vendor": "webpack --config webpack/webpack.vendor.js",
    "webpack:dev": "webpack-dev-server --config webpack/webpack.dev.js  --progress --inline --hot --profile --port=9060",
    "webpack:prod": "yarn run ngc && webpack -p --config webpack/webpack.vendor.js && webpack -p --config webpack/webpack.prod.js",
    "test": "yarn run lint && karma start src/test/javascript/karma.conf.js",
    "test:watch": "karma start --watch",
    "e2e": "yarn run wait-for-managementportal && protractor src/test/javascript/protractor.conf.js",
    "e2e-prod": "yarn run wait-for-managementportal-prod && protractor src/test/javascript/protractor-prod.conf.js",
    "wait-for-managementportal": "wait-port -t 240000 http://localhost:8080/management/health",
    "wait-for-managementportal-prod": "wait-port -t 240000 http://localhost:8080/managementportal/management/health",
    "install:webdriver": "webdriver-manager update --gecko true --standalone false --chrome false"
  }
}
